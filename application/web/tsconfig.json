{
  "compilerOptions": {
    "target": "es2016",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "compilerOptions":{
      "baseUrl": "./",
      "paths":{
        "@": ["src"],
        "@/*": ["src/*"],
      }
    }
  }
}
